<section id="social-section">
    <div class="container">
        <div class="col-lg-12">
            <h2>Keep up to date</h2>
        </div>
        <div class="col-lg-12">
            <div class="twitter-carousel"data-itemsperframe="4" data-autorotate=false data-transitionspeed="300" data-slideduration="4000" data-pagination=true data-rtl=true accessibility="true">
            	{{#each ReTweetUserList}}
	            	<div class="panel">
	            		<div class="feed">
	            			<div class="feed-heading clearfix">
	            				<span>{{createdAt}}</span><span class="twittedby">informaecon</span>
	            			</div>
	            			<p class="message">
								6 reasons why the <a class="hastag">#biosimilars</a> market is exp. to grow from $1.7b to $30b by 2020. <a class="handle">@Frost_Sullivan</a> for @BioProcessOL <a href="https://t.co/K66Uzi4O5x">https://t.co/K66Uzi4O5x</a>
	            			</p>
	            			{{#if Image}}
	            				<div class="twit-img">
	            					<img src="{{Image}}" alt="{{ScreenName}}"/>
	            				</div>
	            			{{/if}}
	            			{{#if Video}}
	            				<a href="videourl" class="video-wrap">
	            					<span class="play-icon"></span>
	            					<img src="{{Image}}" alt="{{ScreenName}}"/>
	            				</a>
	            			{{/if}}
	            		</div>
	            	</div>
	            {{/each}}
            </div>
        </div>
    </div>
</section>