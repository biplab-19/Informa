<div id="product-finder-section" class="product-finder inactive" data-product="true" data-search="false">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <form class="form" method="post" action="#">
                    <fieldset>
                        <legend>Product Finder</legend>
                        <ul class="search-options">
                            <li>
                                <span class="custom-radio">
                                    <label for="SiteSearch" class="label">
                                        <input type="radio" class="custom-radio" checked="checked" name="ProductFinder" id="SiteSearch" data-show="site-search" value="sitesearch" />
                                        <span>Search site</span>
                                    </label>
                                </span>
                                <span class="custom-radio">
                                    <label for="ProductSearch" class="label">
                                              <input type="radio" class="custom-radio" name="ProductFinder" id="ProductSearch" data-show="sector-search" value="productsearch" />
                                              <span>Find Product &amp; services</span>
                                    </label>
                                </span>
                            </li>
                            <li>
                                <a href="#" class="close-finder"><span class="icon-close"><span class="access-link">Close Icon</span></span></a>
                            </li>
                        </ul>
                        <ul class="site-search searchToggle">
                            <li class="menu">
                                <div class="textbox">
                                    <label for="searchField">
                                        Search Here
                                        <input type="text" placeholder="Enter keyword" id="searchField" name="searchtext" />
                                    </label>
                                </div>
                            </li>
                            <li class="button disabled">
                                <button type="submit" class="btn btn-primary" data-submit="sector-search">Go</button>
                            </li>
                        </ul>
                        <ul class="sector-search searchToggle">
                            <li class="menu">
                                <label for="sector-list" class="label-text">Sector:</label>
                                <div class="custom-multiselect">
                                    <select id="sector-list" name="Sectors" class="Sector" multiple="multiple" data-placeholder="Please Select">
                                        <option value="sector1">Commodities</option>
                                        <option value="sector2">Crop protection</option>
                                        <option value="sector3">Diary Products</option>
                                    </select>
                                </div>
                            </li>
                            <li class="menu disabled">
                                <label for="sub-sector-list" class="label-text">Needs:</label>
                                <div class="custom-multiselect">
                                    <select id="sub-sector-list" name="SubSectors" class="SubSector" disabled="disabled" multiple="multiple" data-placeholder="Please Select">
                                        <option value="subsector1">Diary</option>
                                        <option value="subsector2">Meats</option>
                                        <option value="subsector3">Electronics</option>
                                    </select>
                                </div>
                            </li>
                            <li class="menu button">
                                <button type="submit" class="btn btn-primary" data-submit="sector-search">Go</button>
                            </li>
                        </ul>
                    </fieldset>
                </form>
            </div>
        </div>
    </div>
</div>

