  <script> 
    $INFORMA = jQuery.noConflict();

    function openPdf(file) {

// patch for ISW-3125 (PDF Viewer does not always load content)
// viewer will load and re-assign it's currentScaleValue prop as per it's own resize handler
// >>>
// check if object and prop exists
if (PDFViewerApplication) {
  if (PDFViewerApplication.pdfViewer) {
    // keep checking on interval, if viewer is visible
    window.pdfvtimer = setInterval(function () {
      if (jQuery(PDFViewerApplication.pdfViewer.viewer).is(':visible')) {
        // trigger fix after 1s delay and kill interval
        setTimeout(function () {
          if (PDFViewerApplication.pdfViewer.currentScaleValue)
            PDFViewerApplication.pdfViewer.currentScaleValue = PDFViewerApplication.pdfViewer.currentScaleValue;
        }, 100);
        clearInterval(window.pdfvtimer);
      }
    }, 500);
    // kill interval after 30s
    setTimeout(function () {
      clearInterval(window.pdfvtimer);
    }, 30000);
  }
}
      PDFJS.webViewerLoad(file);
      if (navigator.userAgent.indexOf('Edge') != -1) {
        var iframe = document.createElement("iframe");
        iframe.setAttribute("src", file);
        iframe.setAttribute("id", "PDFtoPrint");
        iframe.setAttribute("width", "100%");
        iframe.setAttribute("height", "100%");
        document.getElementById('hiddenIframe').appendChild(iframe);
      }
    }
  </script>
